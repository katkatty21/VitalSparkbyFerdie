{
  "expo": {
    "name": "VitalSpark",
    "slug": "VitalSpark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_VitalSpark_White.png",
    "scheme": "vitalspark",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.vitalspark",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:vitalspark.app"
      ]
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "vitalspark",
              "host": "auth",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.app.vitalspark"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo_VitalSpark_White.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo_VitalSpark.png",
          "imageWidth": 200
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3dbbc2dc-7ea2-41dc-b6f6-8df353d78155"
      }
    }
  }
}
